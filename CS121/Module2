****WARM UP*****
import java.util.Scanner;

public class AverageOfThree
{

    public static void main(String[] args)
    {
        Scanner kbd = new Scanner(System.in);

        System.out.print("Enter the first number: ");
        int userFirstNumber = kbd.nextInt();
        System.out.print("Enter the second number: ");
        int userSecondNumber = kbd.nextInt();
        System.out.print("Enter the third number: ");
        int userThirdNumber = kbd.nextInt();

        double average = ((double) userFirstNumber + userSecondNumber + userThirdNumber) / 3;

        System.out.println("The average is: " + average);

        /* Done wtih Scanner, so close it */
        kbd.close();
    }
}


*****LAB 1****
import java.util.Scanner;

public class FractionConverter
{
    
    public static void main(String[] args)
    {
        Scanner kbd = new Scanner(System.in);

        System.out.print("Enter the numerator: ");
        int numerator = kbd.nextInt();
        System.out.print("Enter the denominator: ");
        int denominator = kbd.nextInt();

        double floatPtEquiv = (numerator / (double) denominator);

        System.out.println("Floating point equivalent: " + floatPtEquiv);

        /* Don't forget to close the Scanner! */
        kbd.close();
    }
}


*****LAB 2*****

import java.util.Scanner;

public class HMSConverter
{
    public static void main(String[] args)
    {
        int hours;
        int minutes;
        int seconds;
        int total;

        Scanner kbd = new Scanner(System.in);

        System.out.print("Enter the number of hours: ");
        hours = kbd.nextInt();

        System.out.print("Enter the number of minutes: ");
        minutes = kbd.nextInt();

        System.out.print("Enter the number of seconds: ");
        seconds = kbd.nextInt();

        System.out.println();

        total = ((hours * 3600) + (minutes * 60) + seconds);

        System.out.println("Total seconds: " + total);
        

        kbd.close();
    }
    
}

Enter the number of hours: 2
Enter the number of minutes: 5
Enter the number of seconds: 19

Total seconds: 7519



*****LAB 3*****

import java.util.Scanner;

public class SecondsConverter
{
    public static void main(String[] args)
    {
        int userSeconds;
        int hours;
        int minutes;
        int seconds;
        
        Scanner kbd = new Scanner(System.in);

        System.out.print("Enter the number of seconds: ");
        userSeconds = kbd.nextInt();
        
        System.out.println();

        hours = userSeconds / 3600;
        System.out.println("Hours: " + hours);

        minutes = userSeconds % 3600 / 60;
        System.out.println("Minutes: " + minutes);

        seconds = userSeconds % 60;
        System.out.println("Seconds: " + seconds);

        kbd.close();
    }

}


Enter the number of seconds: 9999

Hours: 2
Minutes: 46
Seconds: 39



