Lab Activity 2

/**
 * This code will take input from a user and generate a job ad for publication.
 * @author Amanda Halverson
*/

import java.util.Scanner;
import java.text.NumberFormat;

public class AdGenerator
{
    public static void main (String[] args)
    {
        Scanner kbd = new Scanner(System.in);

        // phoneNumber;
        double hourlyRate;


        /* Input processing */
        System.out.println("Welcome to AdGenerator!");
        System.out.println("Please enter your profile information.");
        System.out.println("======================================");

        System.out.print("First name: ");
        String firstName = kbd.nextLine();
        System.out.print("Middle name: ");
        String middleName = kbd.nextLine();
        System.out.print("Last name: ");
        String lastName = kbd.nextLine();
        System.out.print("Job title: ");
        String jobTitle = kbd.nextLine();
        System.out.print("Phone number (10 digit): ");
        String phoneNumber = kbd.nextLine();
        System.out.print("Hourly rate: ");
        hourlyRate = kbd.nextDouble();

        /* Format phone number */

        String phoneNumberFormat = "(" + phoneNumber.substring(0,3) + ") " + phoneNumber.substring(3,6) + "-" + phoneNumber.substring(6);

        /* Get first initial from middle name */

        String fullName = firstName + " " + middleName.charAt(0) + ". " + lastName;

        /* Format hourly rate */

        NumberFormat hourlyRateFormat = NumberFormat.getCurrencyInstance();

        /* Output processing */
        System.out.println("==================================");
        System.out.println("Need a " + jobTitle + "?");
        System.out.println("CALL NOW! " + phoneNumberFormat);
        System.out.println("Ask for " + fullName);
        System.out.println("(Rates start at " + hourlyRateFormat.format(hourlyRate) + "/hr)");
        System.out.println("==================================");

        kbd.close();
    }
    
}

________________________________________________________________________________________________________
Lab Activity 3

/**
 * This code will take the length of the sides of a triangle and calculate the area
 * using Heron's formula.
 * @author Amanda Halverson
 */

import java.util.Scanner;

public class TriangleCalculator
{
    public static void main (String[] args)
    {
        Scanner kbd = new Scanner(System.in);

        /* Input processing */
        System.out.print("Enter the length of side 1: ");
        double sideOne = kbd.nextDouble();
        System.out.print("Enter the length of side 2: ");
        double sideTwo = kbd.nextDouble();
        System.out.print("Enter the length of side 3: ");
        double sideThree = kbd.nextDouble();

        /* Math processing - semiPer is semi-perimeter */
        double semiPer = (sideOne + sideTwo + sideThree) / 2;
        double heronsFormula = Math.sqrt(semiPer * (semiPer - sideOne) * (semiPer - sideTwo)
            * (semiPer - sideThree));

        /* Output processing */
        System.out.print("Area: ");
        System.out.printf("%.3f", heronsFormula);

        kbd.close();
    }
    
}

