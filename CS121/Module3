Lab Activity Warmup

/**
 * This code will generate a new username from user-input first and last names,
 *   including a random 2 digit number.
 * @author Amanda Halverson
 */

import java.util.Scanner;
import java.util.Random;

public class AccountGenerator
{
    public static void main(String[] args)
    {
        Scanner kbd = new Scanner(System.in);
        Random randGen = new Random();

        /* Input processing */
        System.out.print("Enter your first name: ");
        String firstName = kbd.nextLine();
        System.out.print("Enter your last name: ");
        String lastName = kbd.nextLine();

        /* Convert user values to lowercase */
        firstName = firstName.toLowerCase();
        lastName = lastName.toLowerCase();

        /* Get specific characters from user input */
        char firstLetterFirstName = firstName.charAt(0);
        String fiveLettersLastName = lastName.substring(0,5);

        /* Get random int */
        int randInt = randGen.nextInt(90)+10;

        /* Output processing */
        String userName = firstLetterFirstName + fiveLettersLastName;
        System.out.println("Username: " + userName + randInt);

        kbd.close();

    }   
}


________________________________________________________________________________________________________________
Lab Activity 1

/**
 * This code will take a user-inputted radius of a sphere and print the volume and
 *   surface area of the sphere with four decimal precision.
 * @author Amanda Halverson
 */

import java.util.Scanner;

public class SphereCalculator
{
    public static void main(String[] args)
    {
        Scanner kbd = new Scanner(System.in);

        /* Input processing */
        System.out.print("Enter the sphere's radius: ");
        double userRadius = kbd.nextDouble();

        /* Calculations */
        double volume = 4.0 / 3 * Math.PI * Math.pow(userRadius, 3);
        double surfaceArea = 4.0 * Math.PI * Math.pow(userRadius, 2);

        /* Output processing */
        System.out.printf("Volume: " + "%.4f\n", volume);
        System.out.printf("Surface area: " + "%.4f\n", surfaceArea);

        kbd.close();
    }
}

